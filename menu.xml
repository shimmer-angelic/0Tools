<customUI xmlns="http://schemas.microsoft.com/office/2009/07/customui">
	<ribbon>
		<tabs>
			<tab id="customTab" label="场道工具箱" insertAfterMso="TabHelp">
				<group id="gongGongGroup" label="公共">
					<button id="gongGongButton1" label="生成目录" size="normal" onAction="生成工作表目录" getSupertip="插入一个目录工作表，表中为工作簿中所有表的超链接"  />
					<button id="gongGongButton2" label="更新数据源" size="normal" onAction="更新数据源"  />
					<button id="gongGongButton3" label="删除空行" size="normal" onAction="删除要素表空行"  />
					<button id="gongGongButton4" label="编码分组" size="normal" onAction="编码与分组"  />
					<button id="gongGongButton5" label="汇总计算" size="normal" onAction="汇总计算"  />
					<button id="gongGongButton6" label="更新配合比" size="normal" onAction="更新配合比"  />
					<button id="gongGongButton7" label="统一主题" size="normal" onAction="统一主题颜色"  />
					<button id="gongGongButton8" label="生成对比" size="normal" onAction="生成对比表"  />
					<button id="gongGongButton9" label="拆分到行" size="normal" onAction="拆分到行"  />
				</group> 
				<group id="yuSuanGroup" label="预算">
					<button id="yuSuanButton1" label="新建预算" size="normal" onAction="新建预算"  />
					<button id="yuSuanButton2" label="匹配成本" size="normal" onAction="匹配成本项"  />
					<button id="yuSuanButton3" label="设置格式" size="normal" onAction="设置格式"  />
					<button id="yuSuanButton4" label="删成本项" size="normal" onAction="删成本项"  />
					<button id="yuSuanButton5" label="算含钢量" size="normal" onAction="计算分项含钢量"  />
				</group>
				<group id="yueBaoGroup" label="月报">
					<button id="yueBaoButton1" label="新建月报" size="normal" onAction="新建月报"  />
					<button id="yueBaoButton2" label="导入预算" size="normal" onAction="导入预算数据"  />
					<button id="yueBaoButton3" label="设置公式" size="normal" onAction="设置月报公式" />
					<button id="yueBaoButton4" label="更新下拉" size="normal" onAction="更新下拉列表"  />
				</group>
				<group id="suanLiangGroup" label="算量">
					<button id="suanLiangButton1" label="新建计算表" size="normal" onAction="新建计算表"  />
					<button id="suanLiangButton2" label="添加数据" size="normal" onAction="添加数据"  />
					<button id="suanLiangButton3" label="反查数据" size="normal" onAction="反查数据"  />
					<button id="suanLiangButton4" label="求面积" size="normal" onAction="绘图求面积"  />
					<button id="suanLiangButton5" label="设比例" size="normal" onAction="设图形比例"  />
				</group>
			</tab>
		</tabs>
	</ribbon>
</customUI>
